<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demosys.domain.auth.mapper.SysUserProfileMapper">

    <select id="selectByUserId"
            resultType="com.example.demosys.domain.auth.entity.SysUserProfile">
        SELECT
        id,
        user_id,
        phone,
        email,
        title,
        created_at,
        updated_at
        FROM sys_user_profile
        WHERE user_id = #{userId}
    </select>

</mapper>
