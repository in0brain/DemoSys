<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demosys.domain.auth.mapper.SysOrgMapper">

    <select id="selectById"
            resultType="com.example.demosys.domain.auth.entity.SysOrg">
        SELECT
        id,
        org_name,
        org_type
        FROM sys_org
        WHERE id = #{id}
    </select>

</mapper>
